exports[`generateJsx - snapshots > snapshot for 'Interactive Chart' ('interactive') > copyable 1`] = `
"import { Sparkline } from "@microviz/react";

function handleHit(event) {
  const { hit, client } = event.detail;
  const output = document.getElementById("output");
  if (hit) {
    output.textContent = \`Hovered: \${hit.markId} at (\${Math.round(client.x)}, \${Math.round(client.y)})\`;
  } else {
    output.textContent = "Hover over the chart...";
  }
}

export function Chart() {
  return (
    <>
      <Sparkline
  data={[69, 70, 68, 67, 66, 69, 64, 62, 64, 62, 59, 57]}
  width={280}
  height={64}
  interactive
  hitSlop={8}
/>
      <div id="output">Hover over the chart...</div>
    </>
  );
}"
`;

