  </div>
  <div className="card">
    <h2>Delta → Bullet</h2>
    <MicrovizChart
      input={{
        data: { current: 33, max: 37, previous: 15 },
        spec: { type: "sparkline" },
        size: { width: 220, height: 40 },
      }}
    />
  </div>
  <div className="card">
    <h2>Segments → Donut</h2>
    <MicrovizChart
      input={{
        data: [
          { color: "#6366f1", name: "Desktop", pct: 38 },
          { color: "#22c55e", name: "Mobile", pct: 36 },
          { color: "#f59e0b", name: "Tablet", pct: 26 },
        ],
        spec: { type: "sparkline" },
        size: { width: 140, height: 140 },
      }}
    />
  </div>
  <div className="card">
    <h2>Value → Bar</h2>
    <MicrovizChart
      input={{
        data: { max: 20, value: 16 },
        spec: { type: "sparkline" },
        size: { width: 220, height: 32 },
      }}
    />
  </div>
</div>
  );
}"
`;

