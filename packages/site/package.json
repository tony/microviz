    "@microviz/elements": "workspace:*",
    "@microviz/renderers": "workspace:*",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.16.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-live": "^4.1.8",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18"
